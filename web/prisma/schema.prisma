generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}

enum SubmissionState {
  InReview
  Correct
  Incorrect
}

model Submission {
  id      Int             @id @default(autoincrement())
  state   SubmissionState
  message String?
}
